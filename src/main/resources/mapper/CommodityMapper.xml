<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.demo.dao.CommodityDao">
    <select id="getCommodityInfoByCode" parameterType="String" resultType="com.study.demo.entity.Commodity">
        select * from commodity where commoditycode like #{code}
    </select>

</mapper>